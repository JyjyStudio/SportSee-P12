{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["src"],
		"includePattern": ".js",
		"excludePattern": "(node_modules/|docs)"
	},
	"plugins": [
		"plugins/markdown",
		"better-docs/component"
	],
	"opts": {
		"encoding": "utf8",
		"destination": "jsdoc/",
		"recurse": true,
		"template": "./node_modules/better-docs"
	},
	"templates": {
		"better-docs": {
			"name": "My React components"
		}
	}
  }